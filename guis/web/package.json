{
  "name": "Iris",
  "short_name": "Iris - see through the waste",
  "type": "module",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "assemble": "nuxt typecheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "//0": "we need the rm due to https://github.com/vercel/turbo/issues/6823#issuecomment-1951582987 triggered by nuxt/image and firebase",
    "build": "shx rm -rf .output && nuxt build",
    "start": "nuxt start",
    "clean": "nuxt cleanup",
    "test": "vitest -c vitest.unit.config.ts --run",
    "test:int": "vitest -c vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "check": "pnpm install && pnpm test && pnpm test:int && pnpm test:e2e",
    "dev": "nuxt dev",
    "//1": "notice the postinstall hook interacts poorly with prune: https://github.com/pnpm/pnpm/issues/5030",
    "postinstall": "nuxt prepare",
    "//2": "preview should use nuxt preview as below, but until https://github.com/unjs/nitro/issues/749 it is too cumbersome",
    "//3": "cross-env-shell cross-env dotenv -e .env.preview -v NITRO_SSL_CERT=$HOME/.vite-plugin-mkcert/cert.pem NITRO_SSL_KEY=$HOME/.vite-plugin-mkcert/dev.pem nuxt build\" && dotenv -e .env.preview \"cross-env-shell cross-env NITRO_SSL_CERT=$HOME/.vite-plugin-mkcert/cert.pem NITRO_SSL_KEY=$HOME/.vite-plugin-mkcert/dev.pem nuxt preview\"",
    "preview": "cross-env-shell \"dotenv -e .env.preview \\\"nuxt dev --https --ssl-cert $HOME/.vite-plugin-mkcert/cert.pem --ssl-key  $HOME/.vite-plugin-mkcert/dev.pem --port 443\\\"\"",
    "decrypt-creds": "sops -d --output service-account.json service-account.enc.json",
    "production": "pnpm decrypt-creds && pnpm install --frozen-lockfile && dotenv -e .env.production nuxt build && firebase --version && firebase deploy",
    "generate-pwa-assets": "pwa-assets-generator"
  },
  "dependencies": {
    "@electric-sql/client": "0.7.1",
    "@google-cloud/functions-framework": "^3.3.0",
    "@maruware/blob-to-base64": "^0.0.8",
    "@mdi/font": "^7.4.47",
    "@mediapipe/tasks-vision": "^0.10.12",
	"@nuxt/image": "^1.11.0",

    "@nuxtjs/google-fonts": "^3.2.0",
    "@tanstack/query-persist-client-core": "^5.60.5",
    "@tanstack/query-sync-storage-persister": "^5.60.5",
    "@tanstack/vue-query": "^5.60.5",
    "@tensorflow/tfjs-automl": "^1.3.0",
    "@tensorflow/tfjs-backend-webgl": "^4.18.0",
    "@tensorflow/tfjs-core": "^4.18.0",
    "@types/compute-argmax": "^1.0.2",
    "@types/haversine": "^1.1.8",
    "compute-argmax": "^1.0.0",
    "date-fns": "^3.6.0",
    "firebase": "^10.7.1",
    "firebaseui": "^6.1.0",
    "flag-icons": "^7.2.1",
    "haversine": "^1.1.1",
    "intrinsic-scale": "^4.0.0",
    "logrocket": "^7.0.0",
    "nuxt-vuefire": "^1.1.0",
    "pino": "^9.0.0",
    "sharp": "^0.33.3",
    "simple-vue-camera": "^1.1.3",
    "ua-parser-js": "^1.0.37",
    "v-phone-input": "^4.1.2",
    "vue-use-places-autocomplete": "^0.2.2",
    "vue3-google-map": "^0.21.0",
    "vuefire": "^3.1.23",
    "vuetify": "^3.5.17"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.16.0",
    "@eslint/js": "^9.2.0",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxtjs/i18n": "^10.0.4",
    "@playwright/test": "^1.46.0",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vite-pwa/assets-generator": "^1.0.0",
    "@vite-pwa/nuxt": "^1.0.4",
    "nuxt-gtag": "^3.0.3",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.1",
    "eslint": "^9.2.0",
    "eslint-plugin-vue": "^9.26.0",
    "firebase-admin": "^11.10.1",
    "firebase-functions": "^4.4.1",
    "firebase-tools": "^13.29.1",
    "globals": "^15.2.0",
    "nuxt": "^4.0.3",
    "shx": "^0.3.4",
    "simple-git": "^3.24.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^7.9.0",
    "vite-plugin-mkcert": "^1.17.5",
    "vite-plugin-vuetify": "^2.1.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.2",
    "vitest-fail-on-console": "^0.6.3",
    "vue-tsc": "2.0.4"
  }
}
